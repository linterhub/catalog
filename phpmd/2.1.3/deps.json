{
    "$schema": "https://schema.linterhub.com/deps.json",
    "name": "phpmd",
    "dependencies": [
        [
            {
                "manager": "platform",
                "package": "composer"
            },
            {
                "manager": "composer",
                "package": "php",
                "version": ">=5.3.0"
            },
            {
                "manager": "composer",
                "package": "pdepend/pdepend",
                "version": "2.0.*"
            },
            {
                "manager": "composer",
                "package": "symfony/dependency-injection",
                "version": "2.5.*"
            },
            {
                "manager": "composer",
                "package": "symfony/filesystem",
                "version": "2.5.*"
            },
            {
                "manager": "composer",
                "package": "symfony/config",
                "version": "2.5.*"
            },
            {
                "manager": "composer",
                "package": "phpmd/phpmd",
                "version": "2.1.3",
                "target": true
            }
        ]
    ]
}
