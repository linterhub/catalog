{
    "$schema": "https://schema.linterhub.com/deps.json",
    "name": "flake8",
    "dependencies": [
        [
            {
                "manager": "platform",
                "package": "pip"
            },
            {
                "manager": "pip",
                "package": "mccabe",
                "version": ">=0.2.1,<0.6"
            },
            {
                "manager": "pip",
                "package": "pycodestyle",
                "version": "<2.1,>=2.0"
            },
            {
                "manager": "pip",
                "package": "pyflakes",
                "version": "!=1.2.0,>=0.8.1,<1.3,!=1.2.1,!=1.2.2"
            },
            {
                "manager": "pip",
                "target": true
            }
        ]
    ]
}
