{
    "$schema": "https://schema.linterhub.com/deps.json",
    "name": "flake8",
    "dependencies": [
        [
            {
                "manager": "platform",
                "package": "pip"
            },
            {
                "manager": "pip",
                "package": "mccabe",
                "version": "<0.6,>=0.2.1"
            },
            {
                "manager": "pip",
                "package": "pycodestyle",
                "version": ">=2.0,<2.1"
            },
            {
                "manager": "pip",
                "package": "pyflakes",
                "version": ">=0.8.1,<1.3,!=1.2.0,!=1.2.1,!=1.2.2"
            },
            {
                "manager": "pip",
                "target": true
            }
        ]
    ]
}
