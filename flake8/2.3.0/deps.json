{
    "$schema": "https://schema.linterhub.com/deps.json",
    "name": "flake8",
    "dependencies": [
        [
            {
                "manager": "platform",
                "package": "pip"
            },
            {
                "manager": "pip",
                "package": "pyflakes",
                "version": ">=0.8.1"
            },
            {
                "manager": "pip",
                "package": "pep8",
                "version": ">=1.5.7"
            },
            {
                "manager": "pip",
                "package": "mccabe",
                "version": ">=0.2.1"
            },
            {
                "manager": "pip",
                "target": true
            }
        ]
    ]
}
